{% extends "core.twig" %}
{% import 'core/macros.twig' as core %}
{% import 'common/appbar/appbar_nav.twig' as appbar_nav %}

{% set pageType = 'debug' %}

{% set appbarEnabled = true %}
{% set defaultGuideVisibility = true %}
{% set defaultAppbarVisibility = true %}
{% set enableSnapScaling = true %}

{% block foot_scripts %}
<script>
    yt.setConfig('JS_PAGE_MODULES', []);

        yt.setConfig({
    'GUIDED_HELP_LOCALE': "en_US",
    'GUIDED_HELP_ENVIRONMENT': "prod"
    });
</script>
{% endblock %}

{%- block title -%}
{{ Debugger - parent() }}
{% endblock %}

{% block head_css %}
<style id="internals-css">
    #internals-container 
</style>
{% endblock %}

{% block content %}
<div id="internals-sidebar" class="yt-card">
</div>
<div id="internals-container" class="yt-card">
    <div class="internals-header">
        <div class="title">
            {{ yt.internals.title }}
        </div>
    </div>
</div>
{% endblock %}